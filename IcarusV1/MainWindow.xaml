<Window x:Class="IcarusV1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        
        xmlns:local="clr-namespace:IcarusV1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="850">
    <Grid>
        <TextBox x:Name="modelTextBox" HorizontalAlignment="Left" Margin="156,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="14" RenderTransformOrigin="-0.333,0.541"/>
        <Label Content="Drone Model" HorizontalAlignment="Left" Margin="40,101,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.125,0.464" FontSize="14"/>
        <TextBox x:Name="costTextBox" HorizontalAlignment="Left" Margin="156,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="14" RenderTransformOrigin="-0.333,0.541"/>
        <Label Content="Service Problem" HorizontalAlignment="Left" Margin="40,165,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.463,-1.154" FontSize="14"/>
        <TextBox x:Name="nameTextBox" HorizontalAlignment="Left" Margin="156,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="14" RenderTransformOrigin="-0.333,0.541"/>
        <Label Content="Client Name" HorizontalAlignment="Left" Margin="40,71,0,0" VerticalAlignment="Top" FontSize="14"/>
        <TextBox x:Name="problemTextBox" HorizontalAlignment="Left" Margin="156,165,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="14" RenderTransformOrigin="-0.333,0.541" Height="135"/>
        <Label Content="Service Cost" HorizontalAlignment="Left" Margin="40,131,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Service Tag" HorizontalAlignment="Left" Margin="40,304,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.463,-1.154" FontSize="14"/>
        <TextBox HorizontalAlignment="Left" Margin="156,308,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82" FontSize="14" RenderTransformOrigin="-0.333,0.541" Text="UpDown"/>
        <Label Content="Drove Service Application" HorizontalAlignment="Left" Margin="40,24,0,0" VerticalAlignment="Top" FontSize="24" RenderTransformOrigin="0.416,0.526"/>
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="374,45,24,67">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>



    </Grid>
</Window>
